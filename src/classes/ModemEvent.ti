class ModemEvent extends Event {
    side = false;
    channel = false;
    replyChannel = false;
    message = false;
    distance = false;
}

function ModemEvent:__init__( name, side, channel, replyChannel, message, distance )
    self.name = name
    self.channel = channel
    self.replyChannel = replyChannel
    self.message = message
    self.distance = distance

    self.data = { name, side, channel, replyChannel, message, distance }
end
